<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Projects should only be visible to the team members -->
        <record id="project_team_user_rule" model="ir.rule">
            <field name="name">Project Team User Rule</field>
            <field name="model_id" ref="project.model_project_project"/>
            <field name="domain_force">[('team_id.members', 'in', user.id)]</field>
            <field name="groups" eval="False"/>
        </record>
    </data>
</odoo>